import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		double raio = sc.nextDouble();
		double PI = 3.14159;
		
		double Area = PI * Math.pow(raio, 2.0);
		
		System.out.printf("√Årea = %.4f%n", Area);

		
		sc.close();

	}

}
